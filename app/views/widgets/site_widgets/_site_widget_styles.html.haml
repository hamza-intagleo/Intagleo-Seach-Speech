  #aws_suggestions {
    background: #fff;
    position: absolute;
    width: 97%;
    z-index: 10;
  }
  #aws_suggestions ul {
      margin: 0;
      padding: 0;
      list-style: none;
  }
  #aws_suggestions ul li {
      padding: 7px 15px;
      font-weight: 400;
  }
  #aws_suggestions ul li:hover {
    background: #{@btn_clr};
    cursor: pointer;
  }
  #aws_suggestions ul li a{
    color: #{@btn_clr};
    text-decoration: none;
  }
  #aws_suggestions ul li:hover a {
    color: #fff;
  }
  #aws_suggestions ul li.active {
    background: #{@btn_clr};
    cursor: pointer;
  }
  #aws_suggestions ul li.active:hover a {
    color: #fff;
  }
  #aws_suggestions ul li.active a {
    color: #fff;    
  }
          